{% if picture.iconName %}
  {% include 'svg-temp.twig' with {
    iconName: picture.iconName,
    mod: picture.mod
  } %}
{% else %}
  <picture class="picture {{picture.mod}}
  {% if picture.section %} {{picture.section}}__picture{% endif %}
  {% if picture.cover %} picture--cover{% endif %}
  ">
        <source class="picture__source-webp js-source-webp" srcset="{{picture.imgSrc}}.webp" type="image/webp">
      {% if picture.jpg %}
        <source class="picture__source-jpg js-source-jpg" srcset="{{picture.imgSrc}}.jpg" type="image/jpg">
        <img src="{{picture.imgSrc}}.jpg" alt="">
      {% else %}
        <source class="picture__source-png js-source-png" srcset="{{picture.imgSrc}}.png" type="image/png">
        <img class="picture__img js-source-img" src="{{picture.imgSrc}}.png" alt="">
      {% endif %}
  </picture>
{% endif %}
